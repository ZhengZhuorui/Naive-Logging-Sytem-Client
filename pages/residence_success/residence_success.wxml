<!--pages/residence_success/residence_success.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">提交成功</view>
            <view class="weui-msg__desc">您填写的内容已经提交成功！请勿重复提交</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap='shareToOther'>分享</button>
                <button class="weui-btn" type="primaty" bindtap='returnToBegin'>返回</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="../index/index" class="weui-footer__link">大栅栏社区营造基地</navigator>
                </view>
                <view class="weui-footer__text">Copyright © 2016-2018 dashilar</view>
            </view>
        </view>
    </view>
</view>
