<!--pages/preview_mcmiddle/preview_mcmiddle.wxml-->
<view class="page">
    <view class="page__hd">
        <view class="page__title">提交预览</view>
    </view>
    <view class="page__bd">
        <view class="weui-form-preview">
            <view class="weui-form-preview__hd">
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">您所在的组织名称</view>
                    <view class="weui-form-preview__value_in-hd">{{yourorganization}}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">被评估组织名称</view>
                    <view class="weui-form-preview__value_in-hd">{{organization}}</view>
                </view>
            </view>
            <view class="weui-form-preview__bd">
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">符合服务对象需求</view>
                    <view class="weui-form-preview__value">{{neirong}}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">流程清晰且准备充分</view>
                    <view class="weui-form-preview__value">{{liucheng}}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">效果明显且覆盖率大</view>
                    <view class="weui-form-preview__value">{{xiaoguo}}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">组织团结分工明确</view>
                    <view class="weui-form-preview__value">{{fengong}}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">符合预算且财务规范</view>
                    <view class="weui-form-preview__value">{{zijin}}</view>
                </view>
            </view>
            <view class="weui-form-preview__hd">
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">您的建议</view>
                    <view class="weui-form-preview__value_in-hd">{{jianyi}}</view>
                </view>
            </view>
            <view class="weui-form-preview__ft">
                <navigator  class="weui-form-preview__btn weui-form-preview__btn_primary" hover-class="weui-form-preview__btn_active" bindtap='switchBack'>返回</navigator>
            </view>
        </view>
    </view>
</view>

